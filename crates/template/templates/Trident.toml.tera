# For the detailed description of the parameters, see: https://ackee.xyz/trident/docs/latest/features/trident-manifest/

[fuzz]
fuzzing_with_stats = true

[fuzz.coverage]
enable = false
server_port = 58432
loopcount = 0
format = "json"
attach_extension = false

{% for program in programs %}
[[fuzz.programs]]
address = "{{ program.program_id }}"
program = "../target/deploy/{{ program.name }}.so"
{% endfor %} 